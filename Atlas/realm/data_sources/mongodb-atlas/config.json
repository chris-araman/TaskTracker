{
    "name": "mongodb-atlas",
    "type": "mongodb-atlas",
    "config": {
        "clusterName": "Sandbox",
        "readPreference": "primary",
        "sync": {
            "state": "enabled",
            "partition": {
                "key": "_partition",
                "type": "string",
                "required": true,
                "permissions": {
                    "read": {
                        "%%partition": "%%user.id"
                    },
                    "write": {
                        "%%partition": "%%user.id"
                    }
                }
            }
        },
        "wireProtocolEnabled": false
    },
    "version": 1
}
